{
  "name": "vinay-portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://vinayanand3.github.io/vinay-resume",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "npm run deploy:all",
    "build:preview": "VITE_BASE=/vinay-resume/preview/ npm run build",
    "deploy:preview": "VITE_BASE=/vinay-resume/preview/ npm run build && gh-pages -d dist --dest preview",
    "deploy:all": "rm -rf .dist-preview && VITE_BASE=/vinay-resume/preview/ npm run build && mv dist .dist-preview && npm run build && mkdir -p dist/preview && cp -R .dist-preview/* dist/preview/ && gh-pages -d dist"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.1.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
